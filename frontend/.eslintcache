 localStorage.setItem('Account', '');\n      this.state = {\n        LoggedIn:localStorage.LoggedIn,\n        Account:localStorage.Account,\n      }\n    }\n    this.onAccountChange = this.onAccountChange.bind(this)\n  }\n\n  // OnAccountChange \n  onAccountChange(loggedIn, account){\n    this.setState({\n        LoggedIn:loggedIn,\n        Account:account\n    })\n    localStorage.setItem('LoggedIn', loggedIn);\n    console.log(localStorage.LoggedIn)\n    localStorage.setItem('Account', account);\n  }\n\n  render(){\n    return (\n      <>\n        <Navbar accountState={this.state}/>\n        <div className=\"body\">\n          <Switch>\n            <Route exact path=\"/\">\n              <HomePage />  \n            </Route>\n            <Route path=\"/login\">\n              <Login accountHandler={this.onAccountChange} accountState={this.state}/>\n            </Route>\n            <Route path=\"/register\">\n              <Register accountHandler={this.onAccountChange} accountState={this.state}/>\n            </Route>\n            <Route path=\"/profile\">\n              <Profile accountHandler={this.onAccountChange} accountState={this.state}/>\n            </Route>\n            <Route path=\"/logout\">\n              <Logout accountHandler={this.onAccountChange} accountState={this.state}/>\n            </Route>\n            <Route path=\"/landing\">\n              <Landing/>\n            </Route>\n            <Route component={NotFound} />\n          </Switch>\n        </div>\n      </>\n    )\n  }  \n  \n}\n",["63","64"],"/Users/junochiu/Documents/Workspace/Blue_Gear_System/frontend/src/Pages/HomePage.js",[],"/Users/junochiu/Documents/Workspace/Blue_Gear_System/frontend/src/Pages/Register.js",["65","66","67","68"],"import React, { Component } from 'react'\nimport { Redirect } from 'react-router-dom'\nimport { Form, Button, Container, Col } from 'react-bootstrap'\n\nimport \"./components/css/Register.scss\"\nconst axios = require('axios')\n\n\nexport default clas